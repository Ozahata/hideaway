{
  "name": "hideaway",
  "version": "0.3.1",
  "description": "Hideaway middleware for Redux",
  "license": "MIT",
  "homepage": "https://github.com/Ozahata/hideaway",
  "repository": {
    "type": "git",
    "url": "github:Ozahata/hideaway"
  },
  "bugs": "https://github.com/Ozahata/hideaway/issues",
  "keywords": [
    "react",
    "react-redux",
    "redux",
    "thunk",
    "hideaway",
    "middleware",
    "redux-middleware",
    "flux"
  ],
  "author": "Fabiano Tsuneo Maurer Ozahata <makoto.seijitsu@gmail.com>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src test --ext js,ts",
    "clean": "rimraf dist coverage",
    "format:check": "prettier --list-different \"{src,test}/**/*.{js,ts}\" \"**/*.md\"",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run check-types && npm run format:check && npm run lint && npm test",
    "prepack": "npm run clean && webpack --mode=production --progress --profile --colors"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test:coverage"
    }
  },
  "peerDependencies": {
    "redux": "^4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.0",
    "@types/ramda": "^0.27.3",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "prettier": "^2.0.4",
    "redux": "^4.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "ramda": "^0.27.0"
  },
  "npmName": "hideaway",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js",
        "*.d.js"
      ]
    }
  ]
}
