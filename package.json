{
  "name": "hideaway",
  "version": "2.0.0-beta.6.1",
  "description": "Hideaway middleware for Redux",
  "license": "MIT",
  "homepage": "https://github.com/Ozahata/hideaway",
  "repository": {
    "type": "git",
    "url": "github:Ozahata/hideaway"
  },
  "bugs": "https://github.com/Ozahata/hideaway/issues",
  "keywords": [
    "react",
    "react-redux",
    "redux",
    "hideaway",
    "middleware",
    "redux-middleware",
    "flux"
  ],
  "author": "Fabiano Tsuneo Maurer Ozahata <makoto.seijitsu@gmail.com>",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests --ext js,ts",
    "clean": "rimraf dist coverage",
    "format:check": "prettier --list-different \"{src,tests}/**/*.{js,ts}\"",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run check-types && npm run format:check && npm run lint && npm test",
    "prepack:dev": "npm run clean && webpack --mode=development --progress --color --watch",
    "prepack": "npm run clean && webpack --mode=production --progress --color",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "redux": "^4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.3",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-jest": "^27.0.6",
    "eslint": "^7.31.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "redux": "^4.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {},
  "npmName": "hideaway",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js",
        "*.d.js"
      ]
    }
  ]
}
